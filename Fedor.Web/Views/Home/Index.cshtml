@{
    Layout = "~/Views/Shared/BaseLayout.cshtml";
}


<div id="content-wrapper">
    <ul class="slide-content">
        <li id="main" class="slide-action">
            @Html.Action(Actions.Main)
        </li>
        <li id="about" class="slide-action">
            @Html.Action(Actions.About)
        </li>
        <li id="gallery" class="slide-action">
            @Html.Action(Actions.Gallery)
        </li>
        <li id="contacts" class="slide-action">
            @Html.Action(Actions.Contacts)
        </li>
    </ul>
</div>

<script type="text/javascript">
    var baseLayoutView = new BaseLayoutView({ activeTab: "#main" });
</script>


